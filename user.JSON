{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Users Collection Schema",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "format": "email"
    },
    "display_name": {
      "type": "string"
    },
    "photo_url": {
      "type": "string",
      "format": "uri"
    },
    "uid": {
      "type": "string"
    },
    "created_time": {
      "type": "string",
      "format": "date-time"
    },
    "phone_number": {
      "type": "string"
    },
    "Birthday": {
      "type": "string",
      "format": "date"
    },
    "social_media_handle": {
      "type": "string"
    }
  },
  "required": ["email", "uid", "created_time"]
}
